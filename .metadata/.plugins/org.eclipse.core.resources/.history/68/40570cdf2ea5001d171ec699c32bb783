package com;

public class Student implements Comparable<Integer>{

	int id;
	String name;
	int rank;
	
	public Student(int id,String name,int rank)
	{
		this.id=id;
		this.name=name;
		this.rank=rank;
	}
	
	public String toString()
	{
		return id+" - "+name+" : "+rank;
	}

	@Override
	public int compareTo(Integher o) {
		
		return 0;
	}
	
}
