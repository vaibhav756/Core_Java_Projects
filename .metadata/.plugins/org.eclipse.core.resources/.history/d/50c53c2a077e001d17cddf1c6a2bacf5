package com;

import java.io.File;

public class FindingFileAndFolderFromDirectory {

	public static void main(String[] args) {

		File f=new File("C:\\Core Java");
		String[] list = f.list();
		for(String name:list)
		{
		    File f1=new File(f,name);
		    if(f1.isFile())
		    System.out.println(name+" : is File");
		    else
		    	System.out.println(name+" : is File");
		}
	}

}
