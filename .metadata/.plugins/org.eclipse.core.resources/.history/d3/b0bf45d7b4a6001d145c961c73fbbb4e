package com;

import java.util.ArrayList;

public class ComparatorDemo {

	public static void main(String[] args) {
		
		ArrayList<Student> al=new ArrayList<>();
		al.add(new Student(101,"Ram",4));
		al.add(new Student(101,"Ram",4));
		al.add(new Student(101,"Ram",4));
		al.add(new Student(101,"Ram",4));
		al.add(new Student(101,"Ram",4));

	}

}
